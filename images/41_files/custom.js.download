$(document).ready(function(){
    $(".testimonial-slider").owlCarousel({
        items:2,
        loop:true,
        itemsDesktop:[1000,2],
        itemsDesktopSmall:[979,2],
        itemsTablet:[768,1],
        pagination:true,
        autoPlay:true,
        nav:false,
        responsive:{
        0:{
            items:1
        },
        600:{
            items:2
        },
        1000:{
            items:2
        }
    }
    });
});


/*top scroll button*/
var btn = $('#button');

$(window).scroll(function() {
  if ($(window).scrollTop() > 300) {
    btn.addClass('show');
  } else {
    btn.removeClass('show');
  }
});

btn.on('click', function(e) {
  e.preventDefault();
  $('html, body').animate({scrollTop:0}, '300');
});


/*website design*/
$(document).ready(function(){
    $(".website-design-slider").owlCarousel({
        loop:true,
        pagination:true,
        autoplayTimeout:3000,
        autoplay:true,
        nav:false,
        responsive:{
        0:{
            items:1
        },
        600:{
            items:1
        },
        1000:{
            items:1
        }
    }
    });
});



